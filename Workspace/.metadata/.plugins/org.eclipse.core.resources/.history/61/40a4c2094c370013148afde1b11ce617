package general;

public class Test {
	
	Integer[] list;
	
	/*
	 * quicksort
	 */
	public void sort(int start, int end) {
		if(null==list)
			return;
		
		int  i = start;
		int j = end;
		int pivot = list[start + (end - start)/2];
		
		while (i<=j) {
			while(list[i] < pivot) {
				i++;
			}
			while(list[j] > pivot) {
				j--;
			}
			
		}
	}

}
